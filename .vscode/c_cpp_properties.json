{
    "env": {
      "myIncludePath": ["${workspaceFolder}/include", "${workspaceFolder}/src"],
      "myDefines": ["DEBUG", "MY_FEATURE=1"]
    },
    "configurations": [
        {
            "name": "msys2-devkitpro-switch",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/common",
                "${workspaceFolder}/source",
                "c:/msys64/opt/devkitpro/**",
                "c:/msys64/opt/devkitpro/tools/**",
                "c:/msys64/opt/devkitpro/devkitA64/**",
                "c:/msys64/opt/devkitpro/devkitA64/include",
                "c:/msys64/opt/devkitpro/devkitA64/lib/gcc/aarch64-none-elf/14.2.0/include",
                "c:/msys64/opt/devkitpro/libnx/**",
                "c:/msys64/opt/devkitpro/libnx/include",
                "c:/msys64/opt/devkitpro/portlibs/**",
                "c:/msys64/opt/devkitpro/portlibs/switch/include",
                "${workspaceFolder}/lib/libtesla/**",
                "${workspaceFolder}/lib/libtesla/include",
                "${workspaceFolder}/lib/libultra/**",
                "${workspaceFolder}/lib/libultra/include"
            ],
            "defines": [
                "__SWITCH__",
                "NDEBUG",
                "STBTT_STATIC",
                "ULTRA_HPP",
                "TESLA_INIT_IMPL",
                "IRAM_PAYLOAD_MAX_SIZE",
                "IRAM_PAYLOAD_BASE"
            ],
            "windowsSdkVersion": "10.0.22621.2",
            "compilerPath": "c:/msys64/opt/devkitpro/devkitA64/bin/aarch64-none-elf-gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-gcc-arm64"
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
